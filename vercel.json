{
    "devCommand": "uvicorn backend.app:app --host 0.0.0.0 --port 3000",
    "builds": [
        {
            "src": "backend/app.py",
            "use": "@vercel/python"
        }
    ],
    "installCommand": "pip install -r requirements.txt --break-system-packages",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/app.py"
        }
    ]
}